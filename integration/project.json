{
  "name": "integration",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "type": "library",
  "sourceRoot": "integration/src",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "integration/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "coverage": false,
        "jestConfig": "integration/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
